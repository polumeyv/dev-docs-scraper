{
	"name": "docs-scraper",
	"description": "AI-powered documentation scraper built with SvelteKit",
	"private": true,
	"version": "1.0.0",
	"type": "module",
	"scripts": {
		"dev": "npx vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"prepare": "svelte-kit sync || echo ''",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch"
	},
	"devDependencies": {
		"@sveltejs/adapter-auto": "^6.0.1",
		"@sveltejs/kit": "^2.22.4",
		"@sveltejs/vite-plugin-svelte": "^5.1.0",
		"@types/node": "^24.0.14",
		"svelte": "^5.35.5",
		"svelte-check": "^4.2.2",
		"typescript": "^5.8.3",
		"vite": "^6.3.5"
	},
	"dependencies": {
		"@google/generative-ai": "^0.24.1",
		"@tailwindcss/vite": "^4.1.11",
		"@types/uuid": "^10.0.0",
		"cheerio": "^1.1.0",
		"dotenv": "^17.2.0",
		"lucide-svelte": "^0.525.0",
		"socket.io-client": "^4.8.1",
		"tailwindcss": "^4.1.11",
		"uuid": "^11.1.0"
	}
}
